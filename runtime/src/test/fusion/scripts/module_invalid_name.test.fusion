(require "/fusion/experimental/check")


// Not a symbol
(expect_syntax_error (module null  "/fusion" "body"))
(expect_syntax_error (module 12    "/fusion" "body"))

// Symbol with invalid content
(expect_syntax_error (module null.symbol "/fusion" "body"))
(expect_syntax_error (module ''          "/fusion" "body"))
(expect_syntax_error (module $name       "/fusion" "body"))
